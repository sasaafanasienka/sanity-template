{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@sanity/*": ["sanity/*"],
      "@sanity-utils/*": ["sanity-utils/*"],
      "@sections/*": ["src/sections/*"],
      "@shared/*": ["src/shared/*"],
      "@app/*": ["src/app/*"],
      "@utils/*": ["src/utils/*"],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/sections/main-hero/MainHero.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
